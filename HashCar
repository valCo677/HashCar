import java.util.HashMap;

public class Main {
    public static void main( String[] args ) {
     HashMap<Integer,String>map = new HashMap<Integer,String> ();
     map.put(1,"Ferrari");
     map.put(2,"Lamborgini");
     map.put(3,"Mazeratti");
     for(Integer i:map.keySet ()) {
         System.out.println ( i + " " + map.get ( i ) );
     }
    }
}
